<div>
    <h1>Carrito</h1>
        <div>
        {{#each products}}
            <div class="card" style="width: 18rem;">
                <img class="card-img-top" src="{{this.thumbnail}}" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title">{{this.title}}</h5>
                        <p class="card-text">{{this.description}}</p>
                        <p class="card-text">{{this.price}}</p>
                    </div>
            </div>
        {{/each}}
        </div>
</div>